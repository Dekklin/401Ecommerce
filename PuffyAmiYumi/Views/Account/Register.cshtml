@model RegisterViewModel;
@{
    Layout = "_Layout";
}
<h4 class="card-text"> Create an Account </h4>
    <div class="card-img-overlay login-div">
        <form class="card-form" asp-controller="Account" asp-action="Register">
            <input type="text" placeholder="First Name" name="FirstName" />
            <input type="text" placeholder="Last Name" name="LastName" />
            <input type="text" placeholder="Username" name="Username" />
            <input type="password" asp-for="Password" placeholder="Password" name="Password" />
            <input type="password" placeholder="Confirm Password" name="ConfirmPassword" />
            <input type="text" placeholder="Email" name="Email" />
            <input type="date" placeholder="Birthday" name="Birthday" />
            <br />
            <button type="submit" class="btn btn-outline-primary text-dark">Create New Account</button>
        </form>
    </div>
